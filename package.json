{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start:main": "electron .",
    "start:render": "cd renderer && npm run start",
    "dev": "concurrently \"npm run start:render\" \"wait-on http://localhost:3000 && npm run start:main\" ",
    "start": "nodemon --watch main.js --exec npm run start:dev"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.1.2",
    "concurrently": "^5.2.0",
    "electron": "^9.1.2",
    "wait-on": "^5.1.0"
  }
}
